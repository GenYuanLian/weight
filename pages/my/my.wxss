/* index/record.wxss */
@import "../../lib/weui.wxss";
.button-sp-area {
    margin: 0 auto;
    padding-top: 15px;
    width: 60%;
}
.img-box {
  padding-left: 32rpx;
  padding-right: 32rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #e5e5e5;
  position: relative
}
.img-box image {
  width: 686rpx;
  height: 686rpx;
  background-color: #f9f9f9
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 3; 
  width: 100%;
  height: 100%;
  opacity: 0;
}
.canvas-box {
  position: fixed;
  top:999999rpx;
  left: 0
}